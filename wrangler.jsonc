{
    "$schema": "node_modules/wrangler/config-schema.json",
    "name": "anish-land",
    "compatibility_date": "2025-07-28",
    "main": "./dist/_worker.js",
    "assets": {
        "directory": "./dist",
    },
    "observability": {
        "enabled": true
    },
    "send_metrics": false,
    "d1_databases": [
        {
            "binding": "DB",
